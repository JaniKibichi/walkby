<div class="panel panel-default">
      <div class="panel-heading">
        <h3>List of Subscribers</h3>
      </div>
<div class="panel-body">
<table class="table table-hover">
  <thead>
    <tr>
      <th>Status</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subscribes.each do |subscribe| %>
      <tr>
        <td><%= subscribe.status %></td>
        <td><%= subscribe.description %></td>
        <td><%= link_to 'Show', subscribe %></td>
        <td><%= link_to 'Destroy', subscribe, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Subscribers', new_subscribe_path, class: 'btn btn-success' %>
