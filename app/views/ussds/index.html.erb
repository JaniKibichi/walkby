<div class="panel panel-default">
      <div class="panel-heading">
       <h3>List of Ussd Sessions</h3>
      </div>
<div class="panel-body">
<table class="table table-hover">
  <thead>
    <tr>
      <th>Sessionid</th>
      <th>Servicecode</th>
      <th>Phonenumber</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ussds.each do |ussd| %>
      <tr>
        <td><%= ussd.sessionId %></td>
        <td><%= ussd.serviceCode %></td>
        <td><%= ussd.phoneNumber %></td>
        <td><%= link_to 'Show', ussd %></td>
        <td><%= link_to 'Destroy', ussd, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
